# Render Production Environment Variables
NODE_ENV=production
VITE_NODE_ENV=production

# API Configuration
VITE_API_URL=https://backend-gbhz.onrender.com
VITE_WS_URL=wss://backend-gbhz.onrender.com

# 3D Components Configuration
VITE_ENABLE_3D=true
VITE_FALLBACK_3D=true

# Build Configuration
VITE_BUILD_TARGET=production
VITE_SSR_MODE=false

# Performance Configuration
VITE_ENABLE_LAZY_LOADING=true
VITE_ENABLE_CODE_SPLITTING=true

# Security Configuration
VITE_CSP_MODE=production
VITE_ENABLE_CORS=true